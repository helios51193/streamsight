<!DOCTYPE html>
<html>
<head>
  <title>Realtime Events</title>
</head>
<body>
  <h1>Live Events</h1>
  <pre id="log"></pre>

  <script>
    const socket = new WebSocket("ws://127.0.0.1:8000/ws/events/");

    socket.onmessage = function(event) {
      console.log(event)
      const log = document.getElementById("log");
      log.textContent += event.data + "\n";
    };
  </script>
</body>
</html>